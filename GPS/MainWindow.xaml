        <Button x:Name="test" Grid.Column="0" Content="wyznacz &#x0a; dijkstrę" Grid.Row="0" Margin="0,0,3,0" Click="Test_Click" Grid.RowSpan="2" FontSize="30"></Button>
        <Button x:Name="test2" Grid.Column="0" Content="pokaż drogę" Grid.Row="2" Margin="0,0,3,0" Click="Test2_Click" Grid.ColumnSpan="2" FontSize="30"></Button>
